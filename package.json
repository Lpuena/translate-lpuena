{
  "name": "translate-lpuena",
  "displayName": "translate-Lpuena",
  "description": "translate for vscode",
  "version": "0.0.4",
  "icon": "assets/double.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lpuena/translate-lpuena"
  },
  "publisher": "Lpuena",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "translate-lpuena",
      "description": "Configuration for Baidu API",
      "properties": {
        "translate-lpuena.appid": {
          "type": "string",
          "default": "20200921000570318",
          "description": "百度翻译API-appid"
        },
        "translate-lpuena.secret": {
          "type": "string",
          "default": "GLMeRH02ZIQHOCIPlJhW",
          "description": "百度翻译API-secret"
        }
      }
    },
    "keybindings": [
      {
        "command": "translate.inputTranslation",
        "key": "ctrl ctrl",
        "mac": "cmd cmd",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "translate.toChinese",
        "title": "翻译-toChinese"
      },
      {
        "command": "translate.toEnglish",
        "title": "翻译-toEnglish"
      },
      {
        "command": "translate.inputTranslation",
        "title": "翻译-input"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "translate.toChinese",
          "when": "editorFocus && editorHasSelection"
        },
        {
          "command": "translate.toEnglish",
          "when": "editorFocus && editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@types/md5": "^2.3.2",
    "@vitalets/google-translate-api": "^9.2.0",
    "axios": "^1.4.0",
    "md5": "^2.3.0"
  }
}